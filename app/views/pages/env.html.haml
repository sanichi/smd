= render "meta_tags", d: t("pages.env")

%section
  .header
    %h3.d-inline= t("pages.env.title")
      .body
        %table{class: "table table-bordered table-sm"}
          %tbody
            - @sys_info.each do |key, value|
              %tr
                %th= key.to_s.humanize
                %td= value
